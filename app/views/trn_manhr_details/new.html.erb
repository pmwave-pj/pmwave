<h2>工数新規登録画面</h2>

<div class="waku">
  <%= form_for @trn_manhr_detail do |f| %>
      <%= render "form", f: f %>
      <div class="row well" style="margin:20px 0 0 0;padding:10px;">
        <div class="col-sm-12">
          <%= link_to "キャンセル", trn_manhr_details_path, class:"btn btn-default", style:"float:left" %>
          <%= f.submit "登録する", class:"btn btn-primary", style:"float:right" %>
        </div>
      </div>
  <% end %>
</div>



