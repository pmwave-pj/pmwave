<h1>アカウント編集</h1>
<div class="waku">
  <%= form_for @user do |f| %>
      <%= render "form", f: f %>
      <div class="row well" style="margin:20px 0 0 0;padding:10px;">
        <div class="col-sm-12">
          <%= link_to "戻る", request.referer, class:"btn btn-default", style:"float:left" %>
          <%= f.submit '更新', class:"btn btn-primary", style:"float:right" %>
        </div>
      </div>
  <% end %>
</div>